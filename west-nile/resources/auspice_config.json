      "type": "categorical"
    },
    {
      "key": "Isolate Lineage",
      "title": "Strain name",
      "type": "categorical"
    },
    {
      "key": "strain_display",
      "title": "Pretty strain name",
      "type": "categorical"
    },
    {
      "key": "Geographic Region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "Geographic Location",
      "title": "Country",
      "type": "categorical"
    }
  ],
  "display_defaults": {
    "color_by": "clade_membership",
    "map_triplicate": false,
    "distance_measure": "div",
    "tip_label": "strain_display",
